{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": "./src",
        "paths": {
            "_assets/*": ["assets/*"],
            "_components": ["components"],
            "_navigation": ["navigation"],
            "_screens/*": ["screens/*"],
            "_styles/*": ["styles/*"],
            "_utils/*": ["utils/*"],
            "_hooks/utils": ["hooks/utils"],
            "_hooks/utils/*": ["hooks/utils/*"],
            "_types": ["../types.tsx"]
        }
    }
}
